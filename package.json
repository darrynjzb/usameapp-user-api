    "start": "node ./bin/www",
    "local-default-w": "NODE_ENV=local NODE_ENV_CONTEXT=default ./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "eslint-w": "node_modules/.bin/esw app.js ./bin/www ./context/*.js app.js ./app/*.js ./app/**/*.js ./app/**/**/*.js -w --quiet",
